{
  "compilerOptions": {
    "composite": true,
    "declaration": true,
    "declarationMap": false,
    "target": "ESNEXT",
    "lib": ["DOM", "ESNEXT"],
    "module": "CommonJS",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noImplicitAny": false,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": false,
    "strict": true,
    "strictNullChecks": true
  }
}
